@model IEnumerable<Sistema.laboratorio.Paciente>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.PacienteNombre)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.PacienteEmail)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.PacienteTelefono)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.PacienteDni)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.PacienteFechaNac)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.PacienteSexo)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.PacienteNombre)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.PacienteEmail)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.PacienteTelefono)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.PacienteDni)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.PacienteFechaNac)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.PacienteSexo)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.PacienteId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.PacienteId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.PacienteId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
